@model IEnumerable<WebApplication1.Models.Staff>

    <html xmlns="http://www.w3.org/1999/xhtml">
   
    <head>
        <title></title>
        <link href="~/lib/bootstrap/css/bootstrap.css" rel="stylesheet" />
    </head>
    <body>
        <table class="table table-striped table-dark" border="1" align="center">
            <tr>
                <th>Id</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Gender</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th>Birth Day</th>
                <th>Qualification</th>

            </tr>
            @foreach (var o in Model)
            {
                <tr>
                    <td scope="row">@o.Id</td>
                    <td>@o.FirstName</td>
                    <td>@o.LastName</td>
                    <td>@o.Gender</td>
                    <td>@o.Email</td>
                    <td>@o.MobilePhone</td>
                    <td>@o.Date.ToString("MM/dd/yyyy")</td>
                    <td>@o.Qualifications</td>
                </tr>
            }
        </table>
    </body>
    </html>